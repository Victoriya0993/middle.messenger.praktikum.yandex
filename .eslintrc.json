{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "json"
    ],
    "rules": {  
        "no-unused-vars": "off",
        "require-jsdoc": "off",
        "max-len": [1, 120],
        "max-params": [2, 3]
    }
}
